{
  "extends": "airbnb-base",
  "rules": {
    "indent": ["error",  "tab"],
    "no-underscore-dangle": 0,
    "function-paren-newline": ["error", "consistent"],
    "comma-dangle": [
      "error",
      {
        "functions": "ignore"
      }
    ]
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
